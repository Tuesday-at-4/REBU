<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="Anchor_Login" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="github.Tuesday_at_4.REBU.LoginController">
  <children>
    <Label layoutX="270.0" layoutY="93.0" text="LOGIN">
      <font>
        <Font name="System Bold" size="24.0" />
      </font>
    </Label>
    <Label layoutX="94.0" layoutY="141.0" text="USERNAME" />
    <Label layoutX="93.0" layoutY="186.0" stylesheets="@Style.css" text="PASSWORD" />
    <TextField fx:id="txtField_username" layoutX="200.0" layoutY="135.0" prefHeight="27.0" prefWidth="228.0" />
    <PasswordField fx:id="password_textField" layoutX="200.0" layoutY="180.0" prefHeight="27.0" prefWidth="228.0" />
    <Button fx:id="btn_login" layoutX="200.0" layoutY="225.0" mnemonicParsing="false" onMouseClicked="#login" prefHeight="25.0" prefWidth="228.0" text="LOGIN" />
    <Label layoutX="274.0" layoutY="280.0" text="New User?" textFill="WHITE" />
    <Button fx:id="btn_signUp" layoutX="356.0" layoutY="275.0" mnemonicParsing="false" onAction="#signUp" text="SIGN UP" />
  </children>
</AnchorPane>
